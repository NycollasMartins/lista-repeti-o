#include <stdio.h>

int main()
{
    int num, i;
    int a = 0;
    int b = 1;
    int c = 0;

    printf("Insira um numero inteiro maior ou igual a zero: ");
    scanf("%d", &num);

    if (num == 0) {
        c = 0;
    } else if (num == 1) {
        c = 1;
    } else {
        c = 1;
        for (i = 2; i <= num; i++) {
            c = a + b;
            a = b;
            b = c;
        }
    }

    printf("O %dÂº termo da sequencia de Fibonacci eh: %d\n", num, c);

    return 0;
}
